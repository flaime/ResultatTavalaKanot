<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="750.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uiProgram.RootLayoutController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Inställningar">
              <items>
                  <MenuItem mnemonicParsing="false" onAction="#HandelPushaFonster" text="Pusha inställningar" />
                  <MenuItem mnemonicParsing="false" onAction="#handelDatabas" text="Läs från databas" />
                  <MenuItem mnemonicParsing="false" onAction="#HandelläsInInställning" text="Läsa in från PDF (exeperimentiel)" />
              </items>
            </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleAbout" text="Om" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
